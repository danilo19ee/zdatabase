  default = "10.0.0.0/16"
}

variable "VCNname" {
  default = "RitaVCN"
}

variable "Shapes" {
 default = ["VM.Standard.E2.1","VM.Standard.E2.1.Micro","VM.Standard2.1","VM.Standard.E2.1","VM.Standard.E2.2"]
}

variable "OsImage" {
  default = "Oracle-Linux-7.8-2020.05.26-0"
}

variable "webservice_ports" {
  default = ["80","443"]
}

variable "bastion_ports" {
  default = ["22"]
}

vim webserver1_UPDATED.tf >> saida; vim webserver1_UPDATED.tf >> saida;
echo vim webserver2_UPDATED.tf
resource "oci_core_instance" "RitaWebserver2" {
  availability_domain = lookup(data.oci_identity_availability_domains.ADs.availability_domains[0], "name")
  compartment_id = oci_identity_compartment.RitaCompartment.id
  display_name = "RitaWebServer2"
  shape = var.Shapes[0]
  subnet_id = oci_core_subnet.RitaWebSubnet.id
  source_details {
    source_type = "image"
    source_id   = lookup(data.oci_core_images.OSImageLocal.images[0], "id")
  }
  metadata = {
      ssh_authorized_keys = file(var.public_key_oci)
  }
  create_vnic_details {
     subnet_id = oci_core_subnet.RitaWebSubnet.id
     assign_public_ip = false
     nsg_ids = [oci_core_network_security_group.RitaWebSecurityGroup.id]
  }
}

data "oci_core_vnic_attachments" "RitaWebserver2_VNIC1_attach" {
  availability_domain = lookup(data.oci_identity_availability_domains.ADs.availability_domains[0], "name")
  compartment_id = oci_identity_compartment.RitaCompartment.id
  instance_id = oci_core_instance.RitaWebserver2.id
}

data "oci_core_vnic" "RitaWebserver2_VNIC1" {
  vnic_id = data.oci_core_vnic_attachments.RitaWebserver2_VNIC1_attach.vnic_attachments.0.vnic_id
}

output "RitaWebserver2_PrivateIP" {
   value = [data.oci_core_vnic.RitaWebserver2_VNIC1.private_ip_address]
}

