    (SID_DESC =
      (GLOBAL_DBNAME = appscdb)
      (ORACLE_HOME = /u01/app/oracle/product/19.3.0/dbhome_1)
      (SID_NAME = appscdb1)
    )
    (SID_DESC =
      (GLOBAL_DBNAME = appscdb_DGMGRL)
      (ORACLE_HOME = /u01/app/oracle/product/19.3.0/dbhome_1)
      (SID_NAME = appscdb1)
    )
