# Generated by NetworkManager
nameserver 192.168.18.30
search appsdba.info

[root@oraclerac19con1 ~]# chattr +i /etc/resolv.conf

#CONFIGURE HOSTNAME
[root@oraclerac19con1 ~]# hostnamectl set-hostname oraclerac19con1
