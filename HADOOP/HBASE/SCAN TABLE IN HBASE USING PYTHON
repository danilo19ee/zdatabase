########################################
#   SCAN TABLE IN HBASE USING PYTHON   #
########################################


sudo apt-get install python3-pip
pip3 install happybase

python3
import happybase
connection = happybase.Connection('hadoopmaster1', 2181)
table = connection.table('integrasus:saude_digital')
for row in table.scan():
  print(row)

