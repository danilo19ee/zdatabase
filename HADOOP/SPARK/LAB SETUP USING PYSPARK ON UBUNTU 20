#DOWNLOAD LAB DATA
>>> import wget
>>> url = 'https://s3-api.us-geo.objectstorage.softlayer.net/cf-courses-data/CognitiveClass/BD0211EN/data/LabData.zip'
>>> filename = wget.download(url)

#EXTRACT FILE LabData.zip
>>> import zipfile;
>>> zf = zipfile.ZipFile("LabData.zip")
>>> zf.extractall()



